{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/app/env.d.ts",
    "src/renderer/src/**/*",
    "src/shared/**/*",
    "src/preload/*.d.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/renderer/src/*"
      ]
    },
    "jsx": "react-jsx",   
  }
}
