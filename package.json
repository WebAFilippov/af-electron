{
  "type": "module",
  "name": "effectory",
  "author": "Alexey Filippov <msmail2020@yandex.ru>",
  "productname": "Effectory App",
  "description": "Effectory App",
  "version": "0.1.0",
  "main": "./out/main/main.js",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "clean": "rimraf dist out",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run clean && npm run build && electron-builder --win",
    "build:publish": "npm run clean && npm run build && electron-builder --win --publish always",
    "build:publish_never": "npm run clean && npm run build && electron-builder --win --publish never"
  },
  "dependencies": {
